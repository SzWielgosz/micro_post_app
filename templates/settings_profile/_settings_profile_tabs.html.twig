{% embed 'shared/_tabs.html.twig' %}
    {% block tabs %}
        {% embed 'shared/_tab.html.twig' with { route: 'app_settings_profile' } %}
            {% block content %}
                Profile Settings
            {% endblock %}
            {% block path %}
            {{ path('app_settings_profile') }}
            {% endblock %}
        {% endembed %}

        {% embed 'shared/_tab.html.twig' with { route: 'app_settings_profile_picture', classes: 'ml-2' } %}
            {% block content %}
                Profile Picture
            {% endblock %}
            {% block path %}
            {{ path('app_settings_profile_picture')}}
            {% endblock %}
        {% endembed %}
    {% endblock %}
{% endembed %}